---
	import TemplateLayout from '@/layouts/template.astro';
	import Resume from '@/components/resume';
	import { Content } from '@/lib/content';
	
	const data = await Content.getLatestResumeData();
---

<TemplateLayout>
	<Resume {data} />
</TemplateLayout>

<style lang="scss">
	:global(section#resume.resume) {
		display: block;
	}
</style>